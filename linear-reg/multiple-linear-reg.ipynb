   "source": [
    "regr = linear_model.LinearRegression()\n",
    "train_x = train[[\"ENGINESIZE\",\"CYLINDERS\",\"FUELCONSUMPTION_COMB\"]]\n",
    "train_y = train[[\"CO2EMISSIONS\"]]\n",
    "regr.fit(train_x,train_y)\n",
    "print(regr.coef_)"
   ],
   "metadata": {
    "collapsed": false,
    "pycharm": {
     "name": "#%%\n"
    }
